group("vdso-code-header") {
  _vdso_toolchain = "//zircon/kernel:kernel_${target_cpu}"
  if (is_mistos) {
    public_deps = [
      "//zircon/kernel/lib/mistos/userabi:vdso-code-header($_vdso_toolchain)",
    ]
  } else {
    public_deps =
        [ "//zircon/kernel/lib/userabi:vdso-code-header($_vdso_toolchain)" ]
  }
}
